config system virtual-switch
  edit "internal"
    set physical-switch "sw0"
    config port
      delete "internal2"
    end
  next
end

#config system interfaces
#WAN1
{int_WAN1DHCP}
{int_WAN1PPPoE}
#WAN2
{int_WAN2DHCP}
{int_WAN2PPPoE}
#int1
{int_1}
{dhcp_1}
#int2
{int_2}
{dhcp_2}
#int3
{int_3}
{dhcp_3}
#int4
{int_4}
{dhcp_4}
#int5
{int_5}
{dhcp_5}
#intA
{int_A}
{dhcp_A}
#intB
{int_B}
{dhcp_B}
  edit "modem"
      set vdom "root"
      set mode pppoe
      set status down
      set type physical
      set snmp-index 3
  next
  edit "naf.root"
      set vdom "root"
      set type tunnel
      set src-check disable
      set snmp-index 5
  next
  edit "l2t.root"
      set vdom "root"
      set type tunnel
      set snmp-index 12
  next
  edit "ssl.root"
      set vdom "root"
      set type tunnel
      set alias "SSL VPN interface"
      set snmp-index 4
  next
  edit "fortilink"
      set vdom "root"
      set fortilink enable
      set ip 169.254.1.1 255.255.255.0
      set allowaccess ping fabric
      set type aggregate
      set member "a"
      set lldp-reception enable
      set lldp-transmission enable
      set snmp-index 6
  next
  edit "LAN_Clients"
      set vdom "root"
      set ip 172.16.145.1 255.255.255.0
      set allowaccess ping https
      set type hard-switch
      set device-identification enable
      set role lan
      set snmp-index 10
  next
  edit "LAN_Voice"
      set vdom "root"
      set ip 172.16.146.254 255.255.255.0
      set allowaccess ping https
      set type hard-switch
      set device-identification enable
      set role lan
      set snmp-index 11
  next
end

config firewall address
  edit "SSLVPN_TUNNEL_ADDR1"
      set uuid 7ef9198e-7224-51ed-3253-542a4a36c525
      set type iprange
      set associated-interface "ssl.root"
      set start-ip 10.199.1.100
      set end-ip 10.199.1.200
  next
end
config firewall service custom 
edit SMTP_587
set tcp-portrange 587 
next
end
config firewall service group
  edit "G_S_SDAG"
      set member "DNS" "HTTP" "HTTPS" "IKE" "IMAPS" "NTP" "POP3S" "RDP" "SMTP" "SMTP_587"
  next
end
config ips sensor
  edit "sdag_standard_ips"
      set scan-botnet-connections block
      config entries
          edit 1
          next
      end
  next
end
config application list
  edit "sdag_standard_appcontrol"
      set other-application-log enable
      config entries
          edit 1
              set category 2 6
          next
      end
  next
end
