      set block-botnet enable
  next
end
config antivirus settings
  set grayware enable
end
config antivirus profile
  edit "sdag_standard_antivirus"
      config http
          set options scan
          set outbreak-prevention full-archive
      end
      config ftp
          set options scan
          set outbreak-prevention full-archive
      end
      config imap
          set options scan
          set outbreak-prevention full-archive
      end
      config pop3
          set options scan
          set outbreak-prevention full-archive
      end
      config smtp
          set options scan
          set outbreak-prevention full-archive
      end
      config nntp
          set outbreak-prevention full-archive
      end
      config cifs
          set options scan
          set outbreak-prevention full-archive
      end
      config outbreak-prevention
          set ftgd-service enable
      end
  next
end
config webfilter profile
  edit "sdag_standard_webfilter"
      set options block-invalid-url
      config ftgd-wf
          set options error-allow rate-server-ip
          config filters
              edit 26
                  set category 26
                  set action block
              next
              edit 61
                  set category 61
                  set action block
              next
              edit 86
                  set category 86
                  set action block
